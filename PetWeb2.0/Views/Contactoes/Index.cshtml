@model IEnumerable<PetWeb2._0.Models.Contacto>

<p>
    @Html.ActionLink("Nuevo contacto", "Create")
</p>
<table class="table table-hover" >
    <tr class=" table-primary ">
        <th>
            @Html.DisplayNameFor(model => model.Fono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DueñoMascota.Rut)
        </th>
       
    </tr>

@foreach (var item in Model) {
    <tr class="table-secondary">
        <td>
            @Html.DisplayFor(modelItem => item.Fono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DueñoMascota.Rut)
        </td>
      
    </tr>
}

</table>
